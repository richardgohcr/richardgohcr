{
  "name": "richardgohcr",
  "version": "0.0.1",
  "description": "Richard's personal website",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar styles/main.scss -o styles/ --output-style compressed",
    "watch-css": "node-sass-chokidar styles/main.scss -o styles/ --watch --recursive --output-style compressed",
    "start": "concurrently \"node live-server.js\" \"npm:watch-css\" "
  },
