  "bugs": {
    "url": "https://github.com/richardgohcr/richardgohcr/issues"
  },
  "homepage": "https://github.com/richardgohcr/richardgohcr#readme",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^5.1.0",
    "live-server": "^1.2.1",
    "node-sass-chokidar": "^1.4.0"
  }
}
